local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
repeat wait() until game:IsLoaded()
local vu = game:GetService("VirtualUser")
game:GetService("Players").LocalPlayer.Idled:connect(function() vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame) wait(1) vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame) end)
function getWorld()
    for i, v in pairs(workspace.NPCs:GetChildren()) do
        if v:FindFirstChild("NPCSpawn") then
            return v
        end
    end
end
function pushAllNPCs(array)
    if #array == 0 or getWorld() == nil then return end
    for i, v in next, getWorld().NPCSpawn:GetChildren() do
        if table.find(array, v.Name) and v:IsA("Model") and v:FindFirstChild("NPCInfo") and v["NPCInfo"]:FindFirstChild("Health") and v["NPCInfo"].Health.Value > 0 then
            v:PivotTo(game.Players.LocalPlayer.Character:GetPivot() * CFrame.new(1,0,0))
        end
    end
end
function attackMob(array, pushAll)
    if pushAll then pushAllNPCs(array) end
    game.ReplicatedStorage.Remotes.Keybinds.Attack:FireServer()
end
function createHideButton(gui, window)
    local HideButton = Instance.new("ImageButton", gui)
    local UICorner = Instance.new("UICorner", HideButton)
    HideButton.Name = "HideButton"
    HideButton.BackgroundColor3 = Color3.fromRGB(59, 59, 59)
    HideButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
    HideButton.BorderSizePixel = 0
    HideButton.Position = UDim2.new(0.00833767373, 0, 0.107970171, 0)
    HideButton.Selectable = false
    HideButton.Size = UDim2.new(0.0534201749, 0, 0.0980202928, 0)
    HideButton.AutoButtonColor = false
    HideButton.MouseButton1Up:Connect(function()
        window:Minimize()
    end)
end
local Window = Fluent:CreateWindow({
    Title = "Miojo Hub | Anime Souls X",
    SubTitle = "by Miojo Hub",
    TabWidth = 90,
    Size = UDim2.fromOffset(530, 400),
    Acrylic = false,
    Theme = "Dark",
    MinimizeKey = Enum.KeyCode.LeftControl
})
local Tabs = { Farm = Window:AddTab({ Title = "Farm", Icon = "sword" }) }
createHideButton(Fluent.GUI, Window)
Window:SelectTab(1)
Fluent:ToggleTransparency(false)
Fluent.GUI.Name = "MiojoHub"
local Options = Fluent.Options
do
    Tabs.Farm:AddToggle("autoNPC", {Title = "Auto NPC", Default = false })
end
coroutine.resume(coroutine.create(function() while task.wait() and not Fluent.Unloaded do if Options.autoNPC.Value then pushAllNPCs({"Kaskashi"}, true) end end end))
